{
	"name": "thrust",
	"version": "0.2.0",
	"description": "UI components for fast bootstraping of frontend apps",
	"keywords": [
		"vue"
	],
	"author": "Bastien GUIHARD",
	"license": "MIT",
	"main": "dist/thrust.umd.min.js",
	"module": "dist/thrust.common.js",
	"files": [
		"dist/",
		"src/"
	],
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build --target lib --name thrust src/lib.ts",
		"build:app": "vue-cli-service build",
		"test:unit": "vue-cli-service test:unit",
		"lint": "vue-cli-service lint",
		"docs": "npx typedoc --out docs --target es6 --theme minimal --mode file src"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.28",
		"@fortawesome/free-brands-svg-icons": "^5.13.0",
		"@fortawesome/free-solid-svg-icons": "^5.13.0",
		"@fortawesome/vue-fontawesome": "^0.1.9",
		"@types/node": "^11.15.10",
		"rxjs": "^6.5.5",
		"vue": "2.x",
		"vue-class-component": "^7.2.3",
		"vue-property-decorator": "^8.4.1",
		"vue-router": "^3.1.6",
		"vue2-touch-events": "^2.2.1"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^5.13.0",
		"@types/jest": "^24.9.1",
		"@vue/cli-plugin-babel": "^4.3.1",
		"@vue/cli-plugin-typescript": "^4.3.1",
		"@vue/cli-plugin-unit-jest": "^4.3.1",
		"@vue/cli-service": "^4.3.1",
		"@vue/test-utils": "1.0.0-beta.29",
		"compression-webpack-plugin": "^3.1.0",
		"cssnano": "^4.1.10",
		"gitlab-npm-audit-parser": "^1.0.3",
		"node-sass": "^4.14.1",
		"postcss-custom-properties": "^9.1.1",
		"sass-loader": "^8.0.0",
		"tailwindcss": "^1.4.6",
		"tslint-config-airbnb": "^5.11.2",
		"tslint-language-service": "^0.9.9",
		"typedoc": "^0.15.5",
		"typescript": "^3.7.5",
		"vue-template-compiler": "^2.6.10"
	}
}
